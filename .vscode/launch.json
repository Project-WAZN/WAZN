{
    "version": "0.9.9",
    "configurations": [
        {
             "name": "Mainnet: Node",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/builder/output/linux/debug-aes/bin/waznd",
            "args": ["--log-level", "1"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Mainnet: Cli Wallet",
